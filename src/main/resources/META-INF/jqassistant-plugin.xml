<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0">
    <description>Plugin for my language.</description>
    <model>
        <class>com.buschmais.jqassistant.demo.plugin.model.MyLanguageDescriptor</class>
        <class>com.buschmais.jqassistant.demo.plugin.model.MyLanguageFileDescriptor</class>
        <class>com.buschmais.jqassistant.demo.plugin.model.LineDescriptor</class>
        <class>de.uniulm.iai.jqassistant.javasrc.plugin.model.JavaSourceDescriptor</class>
        <class>de.uniulm.iai.jqassistant.javasrc.plugin.model.JavaCompilationUnitDescriptor</class>
    </model>
    <scanner>
        <class>com.buschmais.jqassistant.demo.plugin.scanner.MyLanguageScannerPlugin</class>
        <class>de.uniulm.iai.jqassistant.javasrc.plugin.scanner.JavaSourceScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>
